<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:p="http://www.springframework.org/schema/p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

 <bean name="addrs" class="com.akshay.autowire.xml.Address">
	 <property name="city" value="jaipur"></property>
	 <property name="state" value="Rajasthan"></property>
	 
 </bean>
<bean name="emp" class="com.akshay.autowire.xml.Employe" autowire="byType"></bean>
</beans>